{
    "name": "sqits/laravel-api-skeleton",
    "description": "Package to create a complete Laravel API resource according to stub files.",
    "keywords": ["laravel", "api", "resource", "crud", "skeleton", "artisan"],
    "homepage": "https://github.com/sqits/laravel-api-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Sqits",
            "email": "info@sqits.nl",
            "homepage": "https://www.sqits.nl",
            "role": "Company"
        },
        {
            "name": "Ruud Schaaphuizen",
            "email": "r.schaaphuizen@sqits.nl",
            "homepage": "https://www.sqits.nl",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/sqits/laravel-api-skeleton"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.6"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Sqits\\ApiSkeleton\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sqits\\ApiSkeleton\\ApiSkeletonServiceProvider"
            ]
        }
    }
}
